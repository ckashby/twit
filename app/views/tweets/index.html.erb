<h1>Tweets</h1>


<% @tweets.each do |tweet| %>
      
        <%= link_to tweet.text, tweet_path(tweet) %> | 
        <%= tweet.user_id %> | 
        <%= link_to 'Show', tweet %> |
        <%= link_to 'Edit', edit_tweet_path(tweet) %> |
        <%= link_to 'Destroy', tweet, method: :delete, data: { confirm: 'Are you sure?' } %>
        <hr>

    <% end %>

<br>

<%= link_to 'New Tweet', new_tweet_path, class: "btn btn-large btn-primary" %>
